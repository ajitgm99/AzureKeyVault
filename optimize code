using System;
using System.Text.Json;
using System.Collections.Generic;

public class Program
{
    public static void Main()
    {
        const string jsonData = "{\"db\":\"CVA\",\"collection\":\"CVA NETTINGSET '_FIRM\",\"queryFilterMap\":{\"cobDate\":\"2025-05-02\"},\"selectColumns\":[\"nettingSets.self.creditCurve.curveId\",\"nettingSets.self.fundingCurve.tlv.curveId\",\"nettingSets.counterparty.creditCurve.curveId\"]}";
        
        using JsonDocument doc = JsonDocument.Parse(jsonData);
        var values = ExtractAllValues(doc.RootElement);

        foreach (string value in values)
        {
            Console.WriteLine(value);
        }
    }

    public static List<string> ExtractAllValues(JsonElement element)
    {
        var values = new List<string>();
        ExtractValues(element, values);
        return values;
    }

    private static void ExtractValues(JsonElement element, List<string> values)
    {
        switch (element.ValueKind)
        {
            case JsonValueKind.Object:
                foreach (JsonProperty property in element.EnumerateObject())
                {
                    ExtractValues(property.Value, values);
                }
                break;
                
            case JsonValueKind.Array:
                foreach (JsonElement item in element.EnumerateArray())
                {
                    ExtractValues(item, values);
                }
                break;
                
            default:
                values.Add(element.GetString()!);
                break;
        }
    }
}